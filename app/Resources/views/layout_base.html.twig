    <link rel="stylesheet" href="{{ asset('bundles/app/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}">

    <style>
        @import url('https://fonts.googleapis.com/css?family=Poppins:400,700');

        body {
            font-family: 'Poppins', sans-serif;
        }

        /* H1 - H6 font */
        h1,
        h2,
        h3,
        h4,
        h5,
        h6,
        .h1,
        .h2,
        .h3,
        .h4,
        .h5,

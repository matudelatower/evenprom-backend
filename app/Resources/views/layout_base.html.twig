<!-- blockUI -->
<script src="{{ asset('bundles/util/js/jquery.blockUI.js') }}"></script>
<!-- AdminLTE App -->
<script src="{{ asset('bundles/app/AdminLTE-2.3.5/js/app.min.js') }}"></script>

{# global sets #}
<script>
    twigLocale = "{{ app.request.locale }}";
</script>
<script src="{{ asset('bundles/app/js/app.js') }}"></script>
{% include '::app.js.twig' %}

{% block javascripts %}

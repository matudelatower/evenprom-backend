{% endif %}
<!-- bootstrap time picker -->
<script src="{{ asset('bundles/app/plugins/timepicker/bootstrap-timepicker.min.js') }}"></script>
<!-- Bootstrap WYSIHTML5 -->
<script src="{{ asset('bundles/app/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>

<!DOCTYPE html>
<html>
{% trans_default_domain "app" %}
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ site_name }}| Registrar</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/app/img/favicon.png') }}"/>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="{{ asset('bundles/app/bootstrap/css/bootstrap.css') }}">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="{{ asset('bundles/app/AdminLTE-2.3.5/css/AdminLTE.min.css') }}">
    <style>
        @import url('https://fonts.googleapis.com/css?family=Poppins:400,700');

        body {
            font-family: 'Poppins', sans-serif;
        }

        /* H1 - H6 font */
        h1,
        h2,
        h3,
        h4,
        h5,
        h6,
        .h1,
        .h2,
        .h3,
        .h4,
        .h5,
        .h6 {
            font-family: 'Poppins', sans-serif;
        }

        @media screen and (max-width: 768px) {
            .register-logo img {
                margin-left: 40%;
            }
        }

        @media screen and (min-width: 769px) {
            .register-logo img {
                margin-left: 33%;
            }
        }

        @media screen and (max-width: 620px) {
            .register-logo img {
                margin-left: 33%;
            }
        }

        .register-box {
            margin: 1% auto;
            width: 360px;
        }

    </style>
    <!-- iCheck -->
    <link rel="stylesheet" href="{{ asset('bundles/app/plugins/iCheck/square/blue.css') }}">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition register-page">
<div class="register-box">
    <div class="register-logo">
        <a href="{{ path('app_homepage') }}">
            <img src="{{ asset('bundles/app/img/120x120.png') }}" class="img-responsive">
            <span>{{ site_name }}</span>
        </a>
    </div>

    <div class="register-box-body">

        <p class="login-box-msg">{{ 'register.a.new.empresa'|trans }}</p>

        {{ form_start(form) }}
        {{ form_widget(form) }}
        <div class="row">
            <!-- /.col -->
            <div class="col-xs-4">
                <button type="submit" class="btn btn-primary btn-block btn-flat">{{ 'button.register'|trans }}</button>
            </div>
            <!-- /.col -->
        </div>
        {{ form_end(form) }}

        <div style="margin-top: 3%">
            <a href="{{ path('fos_user_security_login') }}" class="text-center">{{ 'register.already'|trans }}</a>
        </div>
    </div>
    <!-- /.form-box -->
</div>
<!-- /.register-box -->

<!-- jQuery 2.1.4 -->
<script src="{{ asset('bundles/app/plugins/jQuery/jquery-2.2.3.min.js') }}"></script>
<!-- Bootstrap 3.3.5 -->
<script src="{{ asset('bundles/app/bootstrap/js/bootstrap.min.js') }}"></script>
<!-- iCheck -->
<script src="{{ asset('bundles/app/plugins/iCheck/icheck.min.js') }}"></script>
<script>
    $(function () {
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
            increaseArea: '20%' // optional
        });
    });
</script>
</body>
</html>
